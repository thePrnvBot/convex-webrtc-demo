    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "lucide-react": "^0.562.0",
    "motion": "^12.34.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
